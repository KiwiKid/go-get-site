package main

import (
	"strconv"
)

templ process(count LinkCountResult, websiteId string) {
	<div hx-get={ `/process/{websiteId}` } hx-trigger="onload" hx-swap="outerHTML">
		
		<span>processed { strconv.Itoa(count.LinksHavePages) }</span>
		 
		<span>(out of { strconv.Itoa(count.TotalLinks) })</span>
		
	</div>
}


package main

import (
	"strconv"
)

templ process(count LinkCountResult, website Website) {
	<div hx-get={ website.getProcessURL() } hx-trigger="onload" hx-swap="outerHTML">
		<span>processed { strconv.Itoa(count.LinksHavePages) }</span>
		<span>(out of { strconv.Itoa(count.TotalLinks) })</span>
	</div>
}

